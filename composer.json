{
    "name": "codedor/filament-belongs-to-many",
    "description": "HABTM field for Filament",
    "keywords": [
        "codedor",
        "laravel",
        "filament-belongs-to-many"
    ],
    "homepage": "https://github.com/codedor/filament-belongs-to-many",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.17",
        "illuminate/contracts": "^9.0 | ^10.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\BelongsToMany\\": "src",
            "Codedor\\BelongsToMany\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\BelongsToMany\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\BelongsToMany\\Providers\\BelongsToManyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
